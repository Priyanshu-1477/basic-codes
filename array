// largest element in array

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int max=arr[0];
    for(int j=0;j<n;j++){
        if(arr[j]>max) max=arr[j];
    }
    cout<<"largest = "<<max;
}

// TC = o(n)



// largest and second largest element in an array


#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int largest=arr[0];
	int slargest=-1;
	for(int i=0;i<n;i++){
	    if(arr[i]>largest && slargest<arr[i]){
	        slargest=largest;
	        largest=arr[i];
	        }
	   
	     else if(arr[i]!=largest && arr[i]>slargest){
	        slargest=arr[i];}
	    
	    
	}
	cout<<"largest = "<<largest<<endl;
	cout<<"slargest = "<<slargest<<endl;
}	



// to check if the array is sorted(ascending)


#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int ans=0;
	for(int j=0;j<n-1;j++){
	    if(arr[j]>arr[j+1]) {
	        ans=0;
	        break;
	    }
	    else ans=1;
	}
	cout<<ans;

}


















