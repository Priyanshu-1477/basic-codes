// largest element in array

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int max=arr[0];
    for(int j=0;j<n;j++){
        if(arr[j]>max) max=arr[j];
    }
    cout<<"largest = "<<max;
}

// TC = o(n)



// largest and second largest element in an array


#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int largest=arr[0];
	int slargest=-1;
	for(int i=0;i<n;i++){
	    if(arr[i]>largest && slargest<arr[i]){
	        slargest=largest;
	        largest=arr[i];
	        }
	   
	     else if(arr[i]!=largest && arr[i]>slargest){
	        slargest=arr[i];}
	    
	    
	}
	cout<<"largest = "<<largest<<endl;
	cout<<"slargest = "<<slargest<<endl;
}	



// to check if the array is sorted(ascending)


#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int ans=0;
	for(int j=0;j<n-1;j++){
	    if(arr[j]>arr[j+1]) {
	        ans=0;
	        break;
	    }
	    else ans=1;
	}
	cout<<ans;

}



// remove duplicates from the array and show the no. of unique digits



#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int count=1;
	for(int i=0;i<n-1;i++){
	    if(arr[i]!=arr[i+1]){
	        cout<<arr[i]<<" ";
	        count++;
	    }
	}
	cout<<arr[n-1];
	cout<<endl;
	cout<<count;

}




// reverse first p left elements of an array from left


#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int p;
	cin>>p;
	for(int i=0;i<(p/2);i++){
	    swap(arr[i],arr[p-1-i]);
	}
	
	for(int i=0;i<n;i++){
	    cout<<arr[i]<<" ";
	}
	
	

}


// reverse first p elements of an array from right



#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int p;
	cin>>p;
	int j=1;
	for(int i=n-p;i<n-p/2;i++){
	    swap(arr[i],arr[n-j]);
	    j=j+1;
	}
	for(int i=0;i<n;i++){
	    cout<<arr[i]<<" ";
	}

}











