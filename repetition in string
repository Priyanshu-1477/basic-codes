// earliest repeating character
 


#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int p;
	cin>>p;
	int j=1;
	for(int i=n-p;i<n-p/2;i++){
	    swap(arr[i],arr[n-j]);
	    j=j+1;
	}
	for(int i=0;i<n;i++){
	    cout<<arr[i]<<" ";
	}

}




// repeated character present first in a string



#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
	int p;
	cin>>p;
	int j=1;
	for(int i=n-p;i<n-p/2;i++){
	    swap(arr[i],arr[n-j]);
	    j=j+1;
	}
	for(int i=0;i<n;i++){
	    cout<<arr[i]<<" ";
	}

}




